{
  "name": "ESG-Solana-Reporter",
  "version": "0.0.1",
  "description": "ESG Dataspace to Solana Blockchain adapter",
  "author": "482",
  "private": false,
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\\\\\"../src/**/*.ts\\\\\\\" \\\\\\\"../test/**/*.ts\\\\\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm migration:run -- -d ormconfig.ts",
    "migration:generate": "yarn typeorm -- -d  ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:generate:win": "yarn typeorm -- -d ormconfig.ts migration:generate ./src/migrations/%npm_config_name%",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "4.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/swagger": "^5.0.0",
    "@pinata/sdk": "^2.1.0",
    "@solana/spl-token": "^0.4.3",
    "@solana/spl-token-metadata": "^0.1.2",
    "@solana/web3.js": "^1.91.3",
    "axios": "^1.6.8",
    "body-parser": "^1.19.0",
    "bs58": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ngsijs": "1.5.0-rc.0",
    "node-fetch": "^2.7.0",
    "node-jose": "^2.2.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bs58": "^4.0.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
